angular.module 'app', [
  'sc.app.helpers', 'ngRoute', 'ngResource'
]

.config [
  '$routeProvider', '$locationProvider', '$resourceProvider'
  ($routeProvider, $locationProvider, $resourceProvider) ->

    $routeProvider
      .when '/',
        templateUrl: "<%= asset_path("CalculoRecebimento/index") %>"
        controller: 'Cobranca::IndexCtrl'
      .when '/cobrancas',
        templateUrl: "<%= asset_path("CalculoRecebimento/index") %>"
        controller: 'Cobranca::IndexCtrl'

    $locationProvider.html5Mode true
]

.controller 'controll', [
  '$scope', '$filter'
  (sc, $filter)->
]
